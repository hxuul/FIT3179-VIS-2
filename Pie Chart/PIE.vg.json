{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Pie chart displaying male and female data from the gender dataset.",
  "data": {
    "url": "path_to_your_gender_data.csv"
  },
  "transform": [
    {
      "fold": ["Male", "Female"],
      "as": ["Gender", "value"]
    }
  ],
  "mark": "arc",
  "encoding": {
    "theta": {"field": "value", "type": "quantitative"},
    "color": {"field": "Gender", "type": "nominal"}
  },
  "tooltip": [
    {"field": "Gender", "type": "nominal"},
    {"field": "value", "type": "quantitative"}
  ]
}
